{
    "name": "puleeno/nhanh-vn-sdk",
    "description": "PHP SDK cho Nhanh.vn API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Puleeno Nguyen",
            "email": "puleeno@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0",
        "illuminate/collections": "^10.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Puleeno\\NhanhVn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Examples\\": "examples/src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
