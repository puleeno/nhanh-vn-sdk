{
    "name": "nhanh-vn-sdk/examples",
    "description": "Examples for Nhanh.vn SDK v2.0",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "ext-curl": "*",
        "illuminate/collections": "^10.48",
        "monolog/monolog": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Examples\\": "src/",
            "Puleeno\\NhanhVn\\": "../src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "serve": "php -S 127.0.0.1:8000 -t public",
        "start": "php -S 127.0.0.1:8000 -t public"
    }
}
